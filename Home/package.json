        "@types/try-to-catch": "^3.0.0",
        "axios": "^0.27.2",
        "body-parser": "^1.20.0",
        "cli-table": "^0.3.11",
        "Common": "file:../Common",
        "Model": "file:../Model",
        "CommonServer": "file:../CommonServer",
        "compression": "^1.7.4",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "xmlbuilder2": "^3.0.2"
    },
    "resolutions": {},
    "devDependencies": {
        "@types/jest": "^28.1.6",
        "@types/node": "^18.7.3",
        "chrome-launcher": "^0.15.1",
        "commander": "^9.4.0",
        "depcheck": "^1.4.3",
        "jest": "^28.1.3",
        "lighthouse": "^9.6.5",
        "npm-force-resolutions": "0.0.10",
        "ora": "^6.1.2",
        "puppeteer": "^16.1.0",
        "start-server-and-test": "^1.14.0",
        "ts-jest": "^28.0.7",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    }
